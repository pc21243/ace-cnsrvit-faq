{{#project_weeks}}
<div class="week-entry"  data-project-week={{id}}>
  <input type="hidden" name="team_hours_submissions[project_weeks][project_week_id" value={{id}} />
  <div> <h4>Week Start - {{week_start}}</h4> </div>
