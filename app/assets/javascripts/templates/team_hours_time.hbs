    <table class="table col-md-6">
      <tr class="col-md-6">
        <td>
          <table>
            <tr> <td colspan="2"> <div class="label label-default">Sunday</div> </td> </tr>
            <tr>
              <td class="col-md-6"> <span id="day1">{{week_start}} </span> </td>
              <td> <input name="team_hours_submissions[project_weeks][{{id}}][hours_logs][sun]" /> </td>
            </tr>
          </table>
        </td>
      </tr>
      <tr class="col-md-6">
        <td>
          <table>
            <tr> <td colspan="2"> <div class="label label-default">Monday</div> </td> </tr>
            <tr>
              <td class="col-md-6"> <span id="day2">{{mon}} </span> </td>
              <td> <input name="team_hours_submissions[project_weeks][{{id}}][hours_logs][mon]" /> </td>
            </tr>
          </table>
        </td>
      </tr>
      <tr class="col-md-6">
        <td>
          <table>
            <tr> <td colspan="2"> <div class="label label-default">Tuesday</div> </td> </tr>
            <tr>
