            <tr> <td colspan="2"> <div class="label label-default">Saturday</div> </td> </tr>
            <tr>
              <td class="col-md-6"> <span id="day7">{{sat}} </span> </td>
              <td> <input name="team_hours_submissions[project_weeks][{{id}}][hours_logs][sat]" /> </td>
            </tr>
          </table>
        </td>
      </tr>
    </table>
  </div>
</div>
